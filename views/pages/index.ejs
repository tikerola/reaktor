<!DOCTYPE html>
<html lang="en">

<head>
  <%- include ('../partials/head') %>
</head>

<body class="container">

  <!--header>
    <%- include ('../partials/header') %>
  </header-->

  <main class="center">
    <div class="jumbotron">
      <h1>Packages from /var/lib/dpkg/status</h1>
    </div>

    <ul class="list-group">
      <% packages.forEach(function(package) { %>
      <li class="list-group-item"><a href="/package/<%= package %>"><%= package %></a></li>
      <% }); %>
    </ul>
  </main>

  <footer>
    <%- include ('../partials/footer') %>
  </footer>

</body>

</html>